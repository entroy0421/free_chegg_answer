{
    "manifest_version": 2,
  
    "name": "Free chegg answer",
    "description": "This is a google extension for free chegg answer. ",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "https://ajax.googleapis.com/"
    ],
    "content_scripts": [
        {
          "matches": [
            "https://*.chegg.com/*"
          ]
        }
      ]
  }